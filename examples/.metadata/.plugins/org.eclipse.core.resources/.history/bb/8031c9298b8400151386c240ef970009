package family.flowchartmt;

import family.flowchartmt.Literal;
import java.util.Hashtable;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface BoolLit extends EObject, Literal {
  public abstract boolean isValue();
  
  public abstract void setValue(final boolean newValue);
  
  public abstract Object eval(final Hashtable context);
}
