package fsm

import fr.inria.diverse.k3.al.annotationprocessor.Aspect
import fr.inria.diverse.k3.al.annotationprocessor.OverrideAspectMethod

import java.util.ArrayList
import java.util.Hashtable
import java.util.Scanner

import org.eclipse.emf.common.util.EList
import org.eclipse.emf.common.util.BasicEList

import static extension fsm.TransitionAspect.*
import static extension fsm.TriggerAspect.*
import static extension fsm.ExpressionAspect.*
import static extension fsm.ProgramAspect.*
import static extension fsm.StateAspect.*

@Aspect(className=Program)
class ProgramAspect extends StatementAspect {
	
	@OverrideAspectMethod
	def void eval(Hashtable<java.lang.String, Object> context){
		for(Statement _statement : _self.statements){
			_statement.eval(context)
			
		}
	}
}