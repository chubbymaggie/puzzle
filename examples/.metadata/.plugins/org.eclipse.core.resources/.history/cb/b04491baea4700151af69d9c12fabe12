package family.rhapsody.adapters.rhapsodymt;

import family.RhapsodyMT;
import family.rhapsodymt.RhapsodyMTFactory;
import fr.inria.diverse.melange.adapters.ResourceAdapter;
import java.io.IOException;

@SuppressWarnings("all")
public class RhapsodyAdapter extends ResourceAdapter implements RhapsodyMT {
  public RhapsodyAdapter() {
    super(family.rhapsody.adapters.rhapsodymt.RhapsodyMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public RhapsodyMTFactory getFactory() {
    return new family.rhapsody.adapters.rhapsodymt.RhapsodyMTFactoryAdapter() ;
  }
  
  @Override
  public void save(final String uri) throws IOException {
    this.adaptee.setURI(org.eclipse.emf.common.util.URI.createURI(uri));
    this.adaptee.save(null);
  }
}
