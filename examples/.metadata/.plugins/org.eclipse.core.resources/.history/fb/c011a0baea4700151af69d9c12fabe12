package family.logo.adapters.logomt;

import family.logo.adapters.logomt.LogoMTAdaptersFactory;
import family.logomt.Expression;
import family.logomt.ProcCall;
import family.logomt.ProcDeclaration;
import fr.inria.diverse.melange.adapters.EObjectAdapter;
import org.eclipse.emf.common.util.EList;

@SuppressWarnings("all")
public class ProcCallAdapter extends /* EObjectAdapter<kmlogo.ProcCall> */ implements ProcCall {
  private LogoMTAdaptersFactory adaptersFactory;
  
  public ProcCallAdapter() {
    super(family.logo.adapters.logomt.LogoMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public EList<Expression> getActualArgs() {
    return fr.inria.diverse.melange.adapters.EListAdapter.newInstance(adaptee.getActualArgs(), family.logo.adapters.logomt.ExpressionAdapter.class) ;
  }
  
  @Override
  public ProcDeclaration getDeclaration() {
    return adaptersFactory.createProcDeclarationAdapter(adaptee.getDeclaration()) ;
  }
  
  @Override
  public void setDeclaration(final ProcDeclaration o) {
    adaptee.setDeclaration(((family.logo.adapters.logomt.ProcDeclarationAdapter) o).getAdaptee()) ;
  }
}
