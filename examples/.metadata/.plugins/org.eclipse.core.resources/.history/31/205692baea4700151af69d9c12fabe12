package family.rhapsody.adapters.rhapsodymt;

import family.rhapsody.adapters.rhapsodymt.RhapsodyMTAdaptersFactory;
import family.rhapsodymt.Conditional;
import family.rhapsodymt.Expression;
import family.rhapsodymt.Program;
import fr.inria.diverse.melange.adapters.EObjectAdapter;
import java.util.Map;

@SuppressWarnings("all")
public class ConditionalAdapter extends /* EObjectAdapter<rhapsody.Conditional> */ implements Conditional {
  private RhapsodyMTAdaptersFactory adaptersFactory;
  
  public ConditionalAdapter() {
    super(family.rhapsody.adapters.rhapsodymt.RhapsodyMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public Expression getCondition() {
    return adaptersFactory.createExpressionAdapter(adaptee.getCondition()) ;
  }
  
  @Override
  public void setCondition(final Expression o) {
    adaptee.setCondition(((family.rhapsody.adapters.rhapsodymt.ExpressionAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public Program getBody() {
    return adaptersFactory.createProgramAdapter(adaptee.getBody()) ;
  }
  
  @Override
  public void setBody(final Program o) {
    adaptee.setBody(((family.rhapsody.adapters.rhapsodymt.ProgramAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public void eval(final Map context) {
    adaptee.eval(context
    ) ;
  }
  
  @Override
  public void eval(final Map context) {
    adaptee.eval(context
    ) ;
  }
}
