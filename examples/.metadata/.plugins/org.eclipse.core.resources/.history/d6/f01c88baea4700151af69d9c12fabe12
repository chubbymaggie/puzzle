package family.rhapsodymt;

import family.rhapsodymt.AbstractState;
import family.rhapsodymt.Program;
import family.rhapsodymt.Region;
import org.eclipse.emf.common.util.EList;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface State extends EObject, AbstractState {
  public abstract Program getDoActivity();
  
  public abstract void setDoActivity(final Program newDoActivity);
  
  public abstract Program getEntry();
  
  public abstract void setEntry(final Program newEntry);
  
  public abstract Program getExit();
  
  public abstract void setExit(final Program newExit);
  
  public abstract EList<Region> getOwnedRegions();
}
