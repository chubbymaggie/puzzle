package family.flowchartmt;

import family.flowchartmt.Node;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface Arc extends EObject {
  public abstract String getName();
  
  public abstract void setName(final String newName);
  
  public abstract Node getSource();
  
  public abstract void setSource(final Node newSource);
  
  public abstract Node getTarget();
  
  public abstract void setTarget(final Node newTarget);
}
