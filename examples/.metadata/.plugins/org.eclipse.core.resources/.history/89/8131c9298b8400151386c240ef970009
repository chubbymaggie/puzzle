package family.flowchartmt;

import family.flowchartmt.ArithmeticOperator;
import family.flowchartmt.Expression;
import java.util.Hashtable;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface ArithmeticExpression extends EObject, Expression {
  public abstract ArithmeticOperator getOperator();
  
  public abstract void setOperator(final ArithmeticOperator newOperator);
  
  public abstract Expression getLeft();
  
  public abstract void setLeft(final Expression newLeft);
  
  public abstract Expression getRight();
  
  public abstract void setRight(final Expression newRight);
  
  public abstract Object eval(final Hashtable context);
}
