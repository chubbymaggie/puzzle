package family.rhapsody.adapters.rhapsodymt;

import family.rhapsody.adapters.rhapsodymt.RhapsodyMTAdaptersFactory;
import family.rhapsodymt.Program;
import family.rhapsodymt.Region;
import family.rhapsodymt.State;
import family.rhapsodymt.Transition;
import fr.inria.diverse.melange.adapters.EObjectAdapter;
import org.eclipse.emf.common.util.EList;

@SuppressWarnings("all")
public class StateAdapter extends /* EObjectAdapter<rhapsody.State> */ implements State {
  private RhapsodyMTAdaptersFactory adaptersFactory;
  
  public StateAdapter() {
    super(family.rhapsody.adapters.rhapsodymt.RhapsodyMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public String getName() {
    return adaptee.getName() ;
  }
  
  @Override
  public void setName(final String o) {
    adaptee.setName(o) ;
  }
  
  @Override
  public EList<Transition> getIncoming() {
    return fr.inria.diverse.melange.adapters.EListAdapter.newInstance(adaptee.getIncoming(), family.rhapsody.adapters.rhapsodymt.TransitionAdapter.class) ;
  }
  
  @Override
  public EList<Transition> getOutgoing() {
    return fr.inria.diverse.melange.adapters.EListAdapter.newInstance(adaptee.getOutgoing(), family.rhapsody.adapters.rhapsodymt.TransitionAdapter.class) ;
  }
  
  @Override
  public Region getOwnerRegion() {
    return adaptersFactory.createRegionAdapter(adaptee.getOwnerRegion()) ;
  }
  
  @Override
  public void setOwnerRegion(final Region o) {
    adaptee.setOwnerRegion(((family.rhapsody.adapters.rhapsodymt.RegionAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public Program getDoActivity() {
    return adaptersFactory.createProgramAdapter(adaptee.getDoActivity()) ;
  }
  
  @Override
  public void setDoActivity(final Program o) {
    adaptee.setDoActivity(((family.rhapsody.adapters.rhapsodymt.ProgramAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public Program getEntry() {
    return adaptersFactory.createProgramAdapter(adaptee.getEntry()) ;
  }
  
  @Override
  public void setEntry(final Program o) {
    adaptee.setEntry(((family.rhapsody.adapters.rhapsodymt.ProgramAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public Program getExit() {
    return adaptersFactory.createProgramAdapter(adaptee.getExit()) ;
  }
  
  @Override
  public void setExit(final Program o) {
    adaptee.setExit(((family.rhapsody.adapters.rhapsodymt.ProgramAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public EList<Region> getOwnedRegions() {
    return fr.inria.diverse.melange.adapters.EListAdapter.newInstance(adaptee.getOwnedRegions(), family.rhapsody.adapters.rhapsodymt.RegionAdapter.class) ;
  }
}
