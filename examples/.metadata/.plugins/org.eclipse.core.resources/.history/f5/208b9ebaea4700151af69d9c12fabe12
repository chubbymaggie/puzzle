package family.logo.adapters.logomt;

import family.logo.adapters.logomt.LogoMTAdaptersFactory;
import family.logomt.Parameter;
import family.logomt.ParameterCall;
import fr.inria.diverse.melange.adapters.EObjectAdapter;

@SuppressWarnings("all")
public class ParameterCallAdapter extends /* EObjectAdapter<kmlogo.ParameterCall> */ implements ParameterCall {
  private LogoMTAdaptersFactory adaptersFactory;
  
  public ParameterCallAdapter() {
    super(family.logo.adapters.logomt.LogoMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public Parameter getParameter() {
    return adaptersFactory.createParameterAdapter(adaptee.getParameter()) ;
  }
  
  @Override
  public void setParameter(final Parameter o) {
    adaptee.setParameter(((family.logo.adapters.logomt.ParameterAdapter) o).getAdaptee()) ;
  }
}
