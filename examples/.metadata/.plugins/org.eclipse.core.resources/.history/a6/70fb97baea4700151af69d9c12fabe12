package family.logomt;

import family.logomt.Expression;
import family.logomt.ProcDeclaration;
import logo.Context;
import org.eclipse.emf.common.util.EList;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface ProcCall extends EObject, Expression {
  public abstract EList<Expression> getActualArgs();
  
  public abstract ProcDeclaration getDeclaration();
  
  public abstract void setDeclaration(final ProcDeclaration newDeclaration);
  
  public abstract int eval(final Context context);
}
