package family.rhapsodymt;

import family.rhapsodymt.Expression;
import family.rhapsodymt.Program;
import family.rhapsodymt.Statement;
import java.util.Map;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface Conditional extends EObject, Statement {
  public abstract Expression getCondition();
  
  public abstract void setCondition(final Expression newCondition);
  
  public abstract Program getBody();
  
  public abstract void setBody(final Program newBody);
  
  public abstract void eval(final Map context);
}
