package family.logo.adapters.logomt;

import family.logo.adapters.logomt.LogoMTAdaptersFactory;
import family.logomt.Expression;
import family.logomt.Minus;
import fr.inria.diverse.melange.adapters.EObjectAdapter;

@SuppressWarnings("all")
public class MinusAdapter extends /* EObjectAdapter<kmlogo.Minus> */ implements Minus {
  private LogoMTAdaptersFactory adaptersFactory;
  
  public MinusAdapter() {
    super(family.logo.adapters.logomt.LogoMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public Expression getLhs() {
    return adaptersFactory.createExpressionAdapter(adaptee.getLhs()) ;
  }
  
  @Override
  public void setLhs(final Expression o) {
    adaptee.setLhs(((family.logo.adapters.logomt.ExpressionAdapter) o).getAdaptee()) ;
  }
  
  @Override
  public Expression getRhs() {
    return adaptersFactory.createExpressionAdapter(adaptee.getRhs()) ;
  }
  
  @Override
  public void setRhs(final Expression o) {
    adaptee.setRhs(((family.logo.adapters.logomt.ExpressionAdapter) o).getAdaptee()) ;
  }
}
