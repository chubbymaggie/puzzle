package family.logomt;

import family.logomt.Block;
import family.logomt.ControlStructure;
import logo.Context;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface Repeat extends EObject, ControlStructure {
  public abstract Block getBlock();
  
  public abstract void setBlock(final Block newBlock);
  
  public abstract int eval(final Context context);
}
