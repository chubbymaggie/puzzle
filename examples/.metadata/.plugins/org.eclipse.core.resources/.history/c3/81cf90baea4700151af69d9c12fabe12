    return fr.inria.diverse.melange.adapters.EListAdapter.newInstance(adaptee.getStatements(), family.rhapsody.adapters.rhapsodymt.StatementAdapter.class) ;
  }
  
  @Override
  public void eval(final Map context) {
    adaptee.eval(context
    ) ;
  }
  
  @Override
  public void eval(final Map context) {
    adaptee.eval(context
    ) ;
  }
}
