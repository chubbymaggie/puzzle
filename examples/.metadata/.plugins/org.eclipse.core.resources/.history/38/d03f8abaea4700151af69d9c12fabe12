package family.rhapsodymt;

import family.rhapsodymt.AbstractState;
import family.rhapsodymt.Constraint;
import family.rhapsodymt.NamedElement;
import family.rhapsodymt.Statement;
import family.rhapsodymt.Trigger;
import org.eclipse.emf.common.util.EList;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface Transition extends EObject, NamedElement {
  public abstract Trigger getTrigger();
  
  public abstract void setTrigger(final Trigger newTrigger);
  
  public abstract AbstractState getTarget();
  
  public abstract void setTarget(final AbstractState newTarget);
  
  public abstract AbstractState getSource();
  
  public abstract void setSource(final AbstractState newSource);
  
  public abstract EList<Statement> getEffect();
  
  public abstract Constraint getGuard();
  
  public abstract void setGuard(final Constraint newGuard);
}
