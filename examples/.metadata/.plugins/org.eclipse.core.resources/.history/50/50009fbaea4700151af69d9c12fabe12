package family.logo.adapters.logomt;

import family.logo.adapters.logomt.LogoMTAdaptersFactory;
import family.logomt.Back;
import family.logomt.Expression;
import fr.inria.diverse.melange.adapters.EObjectAdapter;

@SuppressWarnings("all")
public class BackAdapter extends /* EObjectAdapter<kmlogo.Back> */ implements Back {
  private LogoMTAdaptersFactory adaptersFactory;
  
  public BackAdapter() {
    super(family.logo.adapters.logomt.LogoMTAdaptersFactory.getInstance()) ;
  }
  
  @Override
  public Expression getSteps() {
    return adaptersFactory.createExpressionAdapter(adaptee.getSteps()) ;
  }
  
  @Override
  public void setSteps(final Expression o) {
    adaptee.setSteps(((family.logo.adapters.logomt.ExpressionAdapter) o).getAdaptee()) ;
  }
}
