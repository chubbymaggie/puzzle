package family.logomt;

import family.logomt.Block;
import family.logomt.ControlStructure;
import logo.Context;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface If extends EObject, ControlStructure {
  public abstract Block getThenPart();
  
  public abstract void setThenPart(final Block newThenPart);
  
  public abstract Block getElsePart();
  
  public abstract void setElsePart(final Block newElsePart);
  
  public abstract int eval(final Context context);
}
