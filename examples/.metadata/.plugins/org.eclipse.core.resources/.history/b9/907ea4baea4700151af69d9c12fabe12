package family.flowchartmt;

import fr.inria.diverse.melange.lib.IFactory;

@SuppressWarnings("all")
public interface FlowchartMTFactory extends IFactory {
  public abstract family.flowchartmt.Flowchart createFlowchart();
  
  public abstract family.flowchartmt.Arc createArc();
  
  public abstract family.flowchartmt.Subflow createSubflow();
  
  public abstract family.flowchartmt.Action createAction();
  
  public abstract family.flowchartmt.Decision createDecision();
  
  public abstract family.flowchartmt.Constraint createConstraint();
  
  public abstract family.flowchartmt.RelationalConstraint createRelationalConstraint();
  
  public abstract family.flowchartmt.Expression createExpression();
  
  public abstract family.flowchartmt.Literal createLiteral();
  
  public abstract family.flowchartmt.IntegerLit createIntegerLit();
  
  public abstract family.flowchartmt.StringLit createStringLit();
  
  public abstract family.flowchartmt.BoolLit createBoolLit();
  
  public abstract family.flowchartmt.ArithmeticExpression createArithmeticExpression();
  
  public abstract family.flowchartmt.RelationalExpression createRelationalExpression();
  
  public abstract family.flowchartmt.VarReference createVarReference();
}
