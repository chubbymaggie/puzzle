package family.logomt;

import family.logomt.Expression;
import logo.Context;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface BinaryExp extends EObject, Expression {
  public abstract Expression getLhs();
  
  public abstract void setLhs(final Expression newLhs);
  
  public abstract Expression getRhs();
  
  public abstract void setRhs(final Expression newRhs);
  
  public abstract int eval(final Context context);
}
