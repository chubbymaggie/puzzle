package family.flowchartmt;

import family.flowchartmt.Constraint;
import family.flowchartmt.Node;
import java.util.Hashtable;
import org.eclipse.emf.ecore.EObject;

@SuppressWarnings("all")
public interface Decision extends EObject, Node {
  public abstract Constraint getGuard();
  
  public abstract void setGuard(final Constraint newGuard);
  
  public abstract void eval(final Hashtable context);
}
