package fr.inria.diverse.melange.ui.vos

class CompositionArc {
	
	// -------------------------------------------------
	// Attributes
	// -------------------------------------------------
	
	public CompositionNode from
	public CompositionNode to
	
	// -------------------------------------------------
	// Methods
	// -------------------------------------------------
	
	def CompositionNode getFrom(){
		return from
	}
	
	def CompositionNode getTo(){
		return to
	}
}