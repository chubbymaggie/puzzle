package es.us.isa.fama;

import es.us.isa.FAMA.Reasoner.Question;
import es.us.isa.FAMA.Reasoner.QuestionTrader;

public class PluginQuestionTrader extends QuestionTrader {

	public PluginQuestionTrader(){
		super("OSGI");
		Class<Question> qcl = (Class<Question>) Class.forName("es.us.isa.FAMA.Reasoner.questions.ProductsQuestion");
		this.questionsMap.put("Products", qcl);
		
	}
}
