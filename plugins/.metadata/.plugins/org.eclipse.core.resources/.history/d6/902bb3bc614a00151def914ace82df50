package fr.inria.diverse.k3.sle.common.tuples;

import org.eclipse.xtext.common.types.JvmDeclaredType;

public class TupleConceptMethodsMembers {

	// -----------------------------------------------
	// Attributes
	// -----------------------------------------------
	
	private JvmDeclaredType concept;
	private TupleMethodMembers methodMembers;
	
	// -----------------------------------------------
	// Constructor
	// -----------------------------------------------
	
	public TupleConceptMethodsMembers(JvmDeclaredType concept, TupleMethodMembers methodMembers){
		this.concept = concept;
		this.methodMembers = methodMembers;
	}

	// -----------------------------------------------
	// Getters and setters
	// -----------------------------------------------
	
	public JvmDeclaredType getConcept() {
		return concept;
	}

	public void setConcept(JvmDeclaredType concept) {
		this.concept = concept;
	}

	public TupleMethodMembers getMethodMembers() {
		return methodMembers;
	}

	public void setMethodMembers(TupleMethodMembers methodMembers) {
		this.methodMembers = methodMembers;
	}
}