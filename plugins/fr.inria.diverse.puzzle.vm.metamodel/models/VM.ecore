<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="vm" nsURI="http://fr.inria.diverse.puzzle.vm.metamodel" nsPrefix="vm">
  <eClassifiers xsi:type="ecore:EClass" name="PNamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PFeatureModel" eSuperTypes="#//PNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="rootFeature" eType="#//PFeature"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraints" upperBound="-1"
        eType="#//PConstraint" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PFeature" eSuperTypes="#//PNamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mandatory" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//PFeature" containment="true" eOpposite="#//PFeature/parent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//PFeature"
        eOpposite="#//PFeature/children"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="groups" upperBound="-1"
        eType="#//PFeatureGroup" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parentGroup" eType="#//PFeatureGroup"
        eOpposite="#//PFeatureGroup/features"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PFeatureGroup">
    <eStructuralFeatures xsi:type="ecore:EReference" name="features" lowerBound="1"
        upperBound="-1" eType="#//PFeature" eOpposite="#//PFeature/parentGroup"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cardinality" lowerBound="1"
        eType="#//PFeatureGroupCardinality" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PFeatureGroupCardinality">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lowerBound" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="upperBound" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PConstraint" eSuperTypes="#//PNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="expression" lowerBound="1"
        eType="#//PBooleanExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PBooleanExpression" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="PFeatureRef" eSuperTypes="#//PBooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="#//PFeature"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PUnaryExpression" eSuperTypes="#//PBooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="expr" lowerBound="1" eType="#//PBooleanExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//PUninaryOperator"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="PUninaryOperator">
    <eLiterals name="not"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PBinaryExpression" eSuperTypes="#//PBooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//PBooleanExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//PBooleanExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//PBinaryOperator"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="PBinaryOperator">
    <eLiterals name="and"/>
    <eLiterals name="or" value="1"/>
    <eLiterals name="implies" value="2"/>
    <eLiterals name="xor" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PLanguageModule" eSuperTypes="#//PNamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="as" eType="#//PAbstractSyntax"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sem" eType="#//PSemantics"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PAbstractSyntax">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ecorePath" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ecoreProject" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PSemantics">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="xtendPath" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="xtendProject" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
