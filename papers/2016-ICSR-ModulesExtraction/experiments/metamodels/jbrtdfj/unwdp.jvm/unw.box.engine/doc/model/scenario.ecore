<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="scenario"
    nsURI="http://scenario/1.0" nsPrefix="scenario">
  <eClassifiers xsi:type="ecore:EClass" name="ScenarioManager">
    <eStructuralFeatures xsi:type="ecore:EReference" name="scheduler" eType="#//BoxScenarioScheduler"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="simulator" eType="#//ScenarioSimulator"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stateManager" eType="#//ModelStateManager"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BoxScenarioScheduler"/>
  <eClassifiers xsi:type="ecore:EClass" name="ScenarioSimulator">
    <eStructuralFeatures xsi:type="ecore:EReference" name="probes" upperBound="-1"
        eType="#//DynTraceSignal"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeSteps" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="breakpts" upperBound="-1"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="debugState" eType="#//BoxScenarioSimDebugState"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelStateManager"/>
  <eClassifiers xsi:type="ecore:EClass" name="DynTraceSignal"/>
  <eClassifiers xsi:type="ecore:EClass" name="DynTracesignalFloat" eSuperTypes="#//DynTraceSignal"/>
  <eClassifiers xsi:type="ecore:EClass" name="DynTraceSignalInt" eSuperTypes="#//DynTraceSignal"/>
  <eClassifiers xsi:type="ecore:EClass" name="DynTraceSignalString" eSuperTypes="#//DynTraceSignal"/>
  <eClassifiers xsi:type="ecore:EClass" name="DynTraceSignalVoltage" eSuperTypes="#//DynTraceSignal"/>
  <eClassifiers xsi:type="ecore:EClass" name="BoxScenarioSimDebugState"/>
</ecore:EPackage>
