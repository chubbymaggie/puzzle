<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="asset" nsURI="http://mmocore.ru/model/asset/1.0.0" nsPrefix="mmocore-asset">
  <eClassifiers xsi:type="ecore:EClass" name="Asset" eSuperTypes="platform:/plugin/ru.mmocore.model/model/mmocore.ecore#//Project">
    <eStructuralFeatures xsi:type="ecore:EReference" name="groups" upperBound="-1"
        eType="#//AssetGroup" containment="true" eOpposite="#//AssetGroup/asset"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AssetGroup" eSuperTypes="platform:/plugin/ru.mmocore.model/model/mmocore.ecore#//Decorate">
    <eStructuralFeatures xsi:type="ecore:EReference" name="asset" eType="#//Asset"
        eOpposite="#//Asset/groups"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//AssetGroup" containment="true" eOpposite="#//AssetGroup/parent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//AssetGroup"
        eOpposite="#//AssetGroup/children"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="-1"
        eType="#//AssetItem" containment="true" eOpposite="#//AssetItem/group"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AssetItem" eSuperTypes="platform:/plugin/ru.mmocore.model/model/mmocore.ecore#//Decorate">
    <eStructuralFeatures xsi:type="ecore:EReference" name="prototype" eType="ecore:EClass platform:/plugin/ru.mmocore.model/model/mmocore.ecore#//Identifiable"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="group" eType="#//AssetGroup"
        eOpposite="#//AssetGroup/items"/>
  </eClassifiers>
</ecore:EPackage>
