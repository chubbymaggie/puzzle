<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="myDsl"
    nsURI="http://www.xtext.org/example/mydsl/MyDsl" nsPrefix="myDsl">
  <eClassifiers xsi:type="ecore:EClass" name="MyDsl">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//BlocoCodigo" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlocoCodigo"/>
  <eClassifiers xsi:type="ecore:EClass" name="RecipienteBlocoCodigo">
    <eStructuralFeatures xsi:type="ecore:EReference" name="bloco" upperBound="-1"
        eType="#//BlocoCodigo" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Termo">
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//Declaracao"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expressao" eSuperTypes="#//BlocoCodigo">
    <eStructuralFeatures xsi:type="ecore:EReference" name="t" eType="#//Termo" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Expressao"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Declaracao" eSuperTypes="#//BlocoCodigo">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Expressao"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Comando" eSuperTypes="#//BlocoCodigo"/>
  <eClassifiers xsi:type="ecore:EClass" name="While" eSuperTypes="#//Comando">
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Expressao"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bloco" eType="#//RecipienteBlocoCodigo"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DoWhile" eSuperTypes="#//Comando">
    <eStructuralFeatures xsi:type="ecore:EReference" name="bloco" eType="#//RecipienteBlocoCodigo"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Expressao"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="If" eSuperTypes="#//Comando">
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Expressao"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bloco" eType="#//RecipienteBlocoCodigo"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="else" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Else">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="exp" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bloco" eType="#//RecipienteBlocoCodigo"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="else" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Print" eSuperTypes="#//Comando">
    <eStructuralFeatures xsi:type="ecore:EReference" name="t" eType="#//Termo" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Read" eSuperTypes="#//Comando">
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//Declaracao"/>
  </eClassifiers>
</ecore:EPackage>
